# Create Table
docker exec -it localstack aws --endpoint="http://localhost:4566" dynamodb create-table --region "sa-east-1" --table-name "customers" --attribute-definitions "AttributeName=licensePlateNumber,AttributeType=S" "AttributeName=identificationDocument,AttributeType=S" --key-schema "AttributeName=licensePlateNumber,KeyType=HASH" "AttributeName=identificationDocument,KeyType=RANGE" --provisioned-throughput "ReadCapacityUnits=5,WriteCapacityUnits=5"